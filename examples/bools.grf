If(True, a, b) -> Inst(a)
If(False, a, b) -> Inst(b)
Inst(&If(cond, a, b)) -> If(Inst(cond), a, b)

// If(cond, a, a) -> a

Not(True) -> False
Not(False) -> True
Not(Not(q)) -> q

And(False, False) -> False
And(False, True) -> False
And(True, False) -> False
And(True, True) -> True

Or(False, False) -> False
Or(False, True) -> True
Or(True, False) -> True
Or(True, True) -> True